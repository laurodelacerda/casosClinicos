<h1>Listing tipodoencas</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipodoencas.each do |tipodoenca| %>
  <tr>
    <td><%= tipodoenca.tipo %></td>
    <td><%= link_to 'Show', tipodoenca %></td>
    <td><%= link_to 'Edit', edit_tipodoenca_path(tipodoenca) %></td>
    <td><%= link_to 'Excluir', tipodoenca, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tipodoenca', new_tipodoenca_path %>
